datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client"
  output   = "../src/generated/client"
}

model Video {
  id            String   @id @default(uuid())
  url           String   @unique
  transcription String?
  translation   String?
  createdAt     DateTime @default(now())
}
